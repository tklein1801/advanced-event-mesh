{
  "name": "unit_simple",
  "dependencies": {
    "@cap-js/advanced-event-mesh": "*"
  },
  "cds": {
    "requires": {
      "messaging": {
        "kind": "advanced-event-mesh",
        "outbox": false,
        "session": {
          "customSessionOpt": true
        },
        "queue": {
          "queueName": "testQueueName",
          "customQueueOpt": true
        },
